# Copy to .env and fill in your values.
#   cp env.example .env

# Required
LLM_API_KEY=sk-your-openai-api-key

# Bot identity
BOT_ADDRESS=ut1_bot_chatgpt_default
BOT_NAME=chatgpt_bot

# Server
NODE_URL=http://localhost:8000
CIS_APP_PUBKEY=ut1_cis_demo_pubkey

# Models
LLM_CHAT_MODEL=gpt-4o
LLM_IMAGE_MODEL=dall-e-3

# Timing
POLL_INTERVAL_S=10
VOTE_RECONSIDER_S=3600

# Set to "false" to skip image generation
ENABLE_IMAGES=true

# Image store â€” run image-store/server.js or use docker compose
IMAGE_STORE_URL=http://localhost:8001
